type Parent {
    id: ID!
    familyName: String!
    givenName: String!
    phoneNumbers:[String]!
    address: String
}

input ParentInput {
    familyName: String!
    givenName: String!
    phoneNumbers:[String]!
    address: String
}
extend type Query {
    getAllParents: [Parent!]!
    getParentById(id: String!): Parent!

}
extend type Mutation{
    addParent(parent: ParentInput!): Parent!
}